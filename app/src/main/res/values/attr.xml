<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IncrementableTextView">
        <attr name="maxVal" format="integer" />
        <attr name="minVal" format="integer" />
        <attr name="incVal" format="float" />
        <attr name="decVal" format="float" />
        <attr name="text" format="string" />
        <attr name="maxLength" format="integer"/>
        <attr name="inputType" format="enum" >
            <enum name="integer" value="0"/>
            <enum name="real" value="1"/>
        </attr>
    </declare-styleable>
</resources>